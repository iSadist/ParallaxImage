{
  "name": "paralax-image",
  "version": "0.1.4",
  "description": "A simple React component to display a parallax image.",
  "author": "Jan Svensson",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/iSadist/ParallaxImage.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
